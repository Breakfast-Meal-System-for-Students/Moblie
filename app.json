{
  "expo": {
    "name": "BMS",
    "slug": "StickerSmash",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/LOGO.png",
    "userInterfaceStyle": "light",
    "scheme": "stickersmash",

    "splash": {
      "image": "./assets/LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xuanthu.StickerSmash"
    },
    "web": {
      "favicon": "./assets/LOGO.png"
    },
    "extra": {
      "eas": {
        "projectId": "43356d33-aa1a-4a6e-bbeb-26bbe1d45734"
      }
    }
  }
}
